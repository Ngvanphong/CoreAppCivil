<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewData["Title"]</title>
    <meta name="description" content="@ViewData["MetaDescription"]">
    <meta name="keywords" content="@ViewData["MetaKeyword"]" />

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="~/client-side/styles/bootstrap4/bootstrap.min.css">
    <link href="~/client-side/plugins/fontawesome-free-5.0.1/css/fontawesome-all.css" rel="stylesheet" type="text/css">
    <link href="~/lib/jquery-ui-1.12.1/jquery-ui.min.css" rel="stylesheet" />

    @*<link rel="stylesheet" type="text/css" href="~/client-side/plugins/OwlCarousel2-2.2.1/owl.carousel.css">
        <link rel="stylesheet" type="text/css" href="~/client-side/plugins/OwlCarousel2-2.2.1/owl.theme.default.css">
        <link rel="stylesheet" type="text/css" href="~/client-side/plugins/OwlCarousel2-2.2.1/animate.css">
        <link rel="stylesheet" type="text/css" href="~/client-side/plugins/slick-1.8.0/slick.css">
        <link rel="stylesheet" type="text/css" href="~/client-side/styles/main_styles.css">
        <link rel="stylesheet" type="text/css" href="~/client-side/styles/responsive.css">*@

    <link href="~/client-side/plugins/OwlCarousel2-2.2.1/bundle.min.css" rel="stylesheet" />
    <link href="~/client-side/plugins/slick-1.8.0/slick.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/client-side/styles/main_styles.css">
    <link href="~/client-side/styles/responsive.css" rel="stylesheet" />


</head>

<body>

    <div class="super_container">

        <!-- Header -->

        <header class="header">
            <!-- Top Bar -->
            <!-- Header Main -->
            @await Component.InvokeAsync("Header")

            <!-- Main Navigation -->

            @await Component.InvokeAsync("MobileMenu")

            <!-- Menu -->
            @await Component.InvokeAsync("MainNavigation")


        </header>

        @RenderBody()

        <!-- Footer -->

        @await Component.InvokeAsync("Footer")
        <!-- Copyright -->

        <div class="copyright">
            <div class="container">
                <div class="row">
                    <div class="col">

                        <div class="copyright_container d-flex flex-sm-row flex-column align-items-center justify-content-start">
                            <div class="copyright_content">
                                <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                                Copyright &copy;
                                <script>document.write(new Date().getFullYear());</script> All rights reserved
                                <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            </div>
                            <div class="logos ml-sm-auto">
                                <ul class="logos_list">
                                    <li><a href="#"><img src="~/client-side/images/logos_1.png" alt=""></a></li>
                                    <li><a href="#"><img src="~/client-side/images/logos_2.png" alt=""></a></li>
                                    <li><a href="#"><img src="~/client-side/images/logos_3.png" alt=""></a></li>
                                    <li><a href="#"><img src="~/client-side/images/logos_4.png" alt=""></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="~/client-side/js/jquery-3.3.1.min.js"></script>
    <script src="~/client-side/styles/bootstrap4/popper.js"></script>
    <script src="~/client-side/styles/bootstrap4/bootstrap.min.js"></script>
    <script src="~/client-side/plugins/greensock/TweenMax.min.js"></script>
    <script src="~/client-side/plugins/greensock/TimelineMax.min.js"></script>
    <script src="~/client-side/plugins/scrollmagic/ScrollMagic.min.js"></script>
    <script src="~/client-side/plugins/greensock/animation.gsap.min.js"></script>
    <script src="~/client-side/plugins/greensock/ScrollToPlugin.min.js"></script>
    <script src="~/client-side/plugins/OwlCarousel2-2.2.1/owl.carousel.js"></script>
    <script src="~/client-side/plugins/easing/easing.js"></script>


    <script src="~/lib/jquery-ui-1.12.1/jquery-ui.min.js"></script>
    <script src="~/app/shared/common.js"></script>
    <script>
        var shareCommons = new shareCommon();
        shareCommons.initilizer();
    </script>

    <!--Start of Tawk.to Script-->
    <script type="text/javascript">
        var Tawk_API = Tawk_API || {}, Tawk_LoadStart = new Date();
        (function () {
            var s1 = document.createElement("script"), s0 = document.getElementsByTagName("script")[0];
            s1.async = true;
            s1.src = 'https://embed.tawk.to/5cc7e9e82846b90c57ac2273/default';
            s1.charset = 'UTF-8';
            s1.setAttribute('crossorigin', '*');
            s0.parentNode.insertBefore(s1, s0);
        })();
    </script>
    <!--End of Tawk.to Script-->

</body>
</html>

@RenderSection("Scripts", required: false)

